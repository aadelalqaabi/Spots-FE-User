{
  "expo": {
    "scheme": "dest",
    "paths":[ "SpotDetails/:id"],
    "name": "Dest",
    "slug": "Dest",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/DestLogo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/DestSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e52b51"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/a4b4c105-4742-46f9-9d80-0ecfd286c07f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification":{
      "icon":"./assets/DestLogo.png"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "3",
      "bundleIdentifier": "com.dest.kuwait",
      "associatedDomains": [
        "applinks:destkw.com"
      ],
      "infoPlist":{
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleDevelopmentRegion":"ar"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/DestLogo.png",
          "color": "#ffffff"
        }
        
      ],["expo-apple-authentication"]
    ],
    "extra": {
      "eas": {
        "projectId": "a4b4c105-4742-46f9-9d80-0ecfd286c07f"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
