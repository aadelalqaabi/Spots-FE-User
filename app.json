{
  "expo": {
    "scheme": "dest",
    "name": "Dest",
    "slug": "Dest",
    "version": "1.3.3",
    "orientation": "portrait",
    "icon": "./assets/DestLogo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/DestSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e52b51"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/a4b4c105-4742-46f9-9d80-0ecfd286c07f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/DestLogo.png",
      "color": "#e52b51"
    },
    "ios": {
      "supportsTablet": false,
      "buildNumber": "19",
      "bundleIdentifier": "com.dest.kuwait",
      "associatedDomains": [
        "applinks:destkw.com"
      ],
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to help you find nearby destinations and provide personalized recommendations.",
        "NSCameraUsageDescription": "We need access to your camera to allow you to take a photo for your profile image and to scan QR codes.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to allow you to select a photo for your profile image.",
        "NSUserNotificationUsageDescription": "Please allow notifications so we can keep you updated with the latest information and relevant updates about our app.",
        "LSApplicationQueriesSchemes": [
          "dest"
        ],
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.dest.kuwait",
      "versionCode": 1,
      "excludeLibraries": [
        "com.google.android.play:core",
        "com.google.android.play:core-common",
        "com.google.android.play:review"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/DestLogo.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-apple-authentication"
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "a4b4c105-4742-46f9-9d80-0ecfd286c07f"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
