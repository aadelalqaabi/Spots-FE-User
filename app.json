{
  "expo": {
    "scheme": "dest",
    "name": "Dest",
    "slug": "Dest",
    "version": "1.2.2",
    "orientation": "portrait",
    "icon": "./assets/DestLogo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/DestSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e52b51"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/a4b4c105-4742-46f9-9d80-0ecfd286c07f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/DestLogo.png",
      "color": "#e52b51"
    },
    "ios": {
      "supportsTablet": false,
      "buildNumber": "13",
      "bundleIdentifier": "com.dest.kuwait",
      "associatedDomains": [
        "applinks:destkw.com"
      ],
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "NSLocationWhenInUseUsageDescription": "We use your location to help you find the perfect Dest near you",
        "NSCameraUsageDescription": "Please allow camera access so you can add your image",
        "NSPhotoLibraryUsageDescription": "Please allow library access so you can add your image",
        "NSUserNotificationUsageDescription": "Please allow notification so we can give you updates on everything",
        "LSApplicationQueriesSchemes": ["dest"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/DestLogo.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-apple-authentication"
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "a4b4c105-4742-46f9-9d80-0ecfd286c07f"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
